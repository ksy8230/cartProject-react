        "ecmaVersion": 2019,
        "sourceType": "module", // import, export
        "ecmaFeatures": {
            "jsx" : true,
        }
    },
    "env" : {
        "browser": true,
        "node" : true,
    },
    "extends": [
        "eslint: recommended",
        "plugin: react/recommended",
    ],
    "plugins": [
        "import",
        "react-hooks",
    ]
}